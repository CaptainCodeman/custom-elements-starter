    "serve-static": "^1.14.1",
    "sinon": "^7.4.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "wct-mocha": "^1.0.1",
    "web-component-tester": "^6.9.2"
  }
