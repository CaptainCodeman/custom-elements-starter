    "@material/mwc-icon": "^0.6.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.1"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/puppeteer": "^1.19.0",
    "@types/sinon": "^7.0.13",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "cryptiles": ">=4.1.2",
    "express": "^4.17.1",
    "express-cache-headers": "^0.1.4",
    "express-transform-bare-module-specifiers": "^1.0.2",
    "lodash": ">=4.17.13",
